{
    "name": "filter-pipe/filter-pipe",
    "type": "library",
    "autoload": {
        "psr-4": {
            "FilterPipe\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "laravel/sail": "^1.15"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilterPipe\\FilterPipeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
